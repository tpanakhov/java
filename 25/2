import java.util.Scanner;
import java.util.regex.Pattern;

public class Task2 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Введите строку для проверки: ");
        String input = scanner.nextLine();
        
        // Регулярное выражение из задания
        String pattern = "^abcdefghijklmnopqrstuv18340$";
        
        if (Pattern.matches(pattern, input)) {
            System.out.println("Строка соответствует 'abcdefghijklmnopqrstuv18340'");
        } else {
            System.out.println("Строка НЕ соответствует 'abcdefghijklmnopqrstuv18340'");
        }
        
        scanner.close();
    }
}
